{
  "name": "markso-vibe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
  "lint": "next lint",
  "bdd:logic": "cucumber-js --require-module ts-node/register --require features/step_definitions/**/*.ts --tags @logic features/**/*.feature",
  "bdd:ui": "start-server-and-test dev http://localhost:3000 'cucumber-js --require-module ts-node/register --require features/step_definitions/**/*.ts --tags @ui features/**/*.feature'",
  "bdd": "npm run bdd:logic && npm run bdd:ui"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "next": "15.4.6"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
  "@eslint/eslintrc": "^3",
  "@cucumber/cucumber": "^11",
  "ts-node": "^10.9.2",
  "playwright": "^1.48.0",
  "start-server-and-test": "^2.0.3"
  }
}
